print("enter data to form contingency table")
a1<-scan()
a2<-scan()
a3<-scan()
a4<-scan()
a<-as.table(rbind(a1,a2,a3,a4))
dimnames(a)=list(Empcategory=c("Labour","Clerks","Technicians","Executives"), BonusSchemes=c("Type1","Type2","Type3"))
print(a)
csum<-colSums (a)
rsum<-rowSums (a)
mytable<-(rbind(a,csum))
mytable<-(cbind(a,rsum))
print(mytable)
test<-chisq.test(a)
print(test)
print(test$expected,4)
